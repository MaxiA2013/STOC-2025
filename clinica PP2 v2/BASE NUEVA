  PRIMARY KEY (`id_persona`))
ENGINE = InnoDB
DEFAULT CHARACTER SET = utf8;


-- -----------------------------------------------------
-- Table `clinica`.`usuario`
-- -----------------------------------------------------
DROP TABLE IF EXISTS `clinica`.`usuario` ;

CREATE TABLE IF NOT EXISTS `clinica`.`usuario` (
  `id_usuario` INT NOT NULL AUTO_INCREMENT,
  `nombre_usuario` VARCHAR(500) NULL DEFAULT NULL,
  `email` VARCHAR(500) NULL DEFAULT NULL,
  `password` VARCHAR(500) NULL DEFAULT NULL,
  `estado` TINYINT(1) NOT NULL,
  `ultima_conexion` DATETIME NULL DEFAULT NULL,
  `fecha_alta` DATETIME NULL DEFAULT NULL,
  `fecha_baja` DATETIME NULL DEFAULT NULL,
  `persona_id_persona` INT NOT NULL,
  PRIMARY KEY (`id_usuario`),
  INDEX `fk_usuario_persona1_idx` (`persona_id_persona` ASC) ,
  CONSTRAINT `fk_usuario_persona1`
    FOREIGN KEY (`persona_id_persona`)
    REFERENCES `clinica`.`persona` (`id_persona`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB
DEFAULT CHARACTER SET = utf8;


