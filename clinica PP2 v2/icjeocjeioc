    FOREIGN KEY (`clinica_id_clinica`)
    REFERENCES `clinica`.`clinica` (`id_clinica`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION)
ENGINE = InnoDB;


-- -----------------------------------------------------
-- Table `clinica`.`agendar-cita`
-- -----------------------------------------------------
DROP TABLE IF EXISTS `clinica`.`agendar-cita` ;

